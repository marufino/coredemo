<div class="col-xs-12 shadow user-profile" style="margin-bottom: 30px">
    <div class="col-xs-3 user-profile">
      <%= image_tag("profile_thumb.png", size: "145x125", :alt => "rss feed", :style => "padding-left: 20px" ) %>
    </div>
    <div class="col-xs-3 user-profile">
      <b><%= user.first_name %> <%= user.last_name %></b><br>
      <small><%= user.title %></small><br>

      <small>Assignment Date: <%= score.assigned_date %></small><br>

      <small>Previous evaluation: <%= user.meta.previous_scorecard(score).assigned_date %></small><br>

      <small>Evaluated by:</small>
        <% @observers.each do |o| %>
          <small><%= o.user.first_name %> <%= o.user.last_name %></small><br>
        <% end %>
      <small>Result:</small><small><%=user.meta.previous_scorecard(score).total%></small><br>
    </div>
    <div class = "col-xs-4">
      Project: <%= Project.find(@score.assignment.project_id).name %><br>
      Date: <%= @score.assigned_date %><br>
      Observer: <%= current_user.first_name %> <%= current_user.last_name %><br>
      Areas of Strength:
      <% score.area_of_strength.competencies.each do |c| %>
        <%= c.name %>
      <% end %><br>
      Areas of Weakness:
      <% score.area_of_weakness.competencies.each do |c| %>
          <%= c.name %>
      <% end %>

    </div>
    <div class = "col-xs-2">
      <h1 style="color: greenyellow"><strong><%= score.total %></strong></h1>
      <h4>CORE Score</h4>
    </div>
</div>
