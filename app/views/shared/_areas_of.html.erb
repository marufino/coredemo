<div class="col-xs-12 shadow user-profile" style="margin-bottom: 50px;">

  <div class = 'row' style = 'margin-left: 30px'>
    <div class="col-xs-3 user-profile">
        <h4>Areas of Strength</h4>
    </div>
    <div class="col-xs-9">
      <% score.area_of_strength.competencies.each do |c| %>
        <div class = 'col-xs-4 btn btn-success'><%= c.name %></div>
      <% end %>
    </div>
  </div>
  <div class = 'row' style = 'margin-left: 30px'>
    <div class="col-xs-3 user-profile">
      <h4>Areas of Weakness</h4>
    </div>
    <div class="col-xs-9">
      <% score.area_of_weakness.competencies.each do |c| %>
          <div class = 'col-xs-4 btn btn-danger'><%= c.name %></div>
      <% end %>
    </div>
  </div>


</div>