<div class = "navbar-default sidebar" role="navigation">
    <div class = "sidebar-nav navbar-collapse">
      <ul class="nav in" id="side-menu">
        <h4>Michigan & Manchester</h4>
        <p>CORE Training Web App</p>
      </ul>
        <ul class="nav in" id="side-menu">
          <li><%= link_to "Overview", new_user_registration_path %></li>
          <li><%= link_to "View Results", new_user_registration_path %></li>
          <li><%= link_to "Conduct a Survey", new_user_registration_path %></li>
          <li><%= link_to "Administration", new_user_registration_path %></li>
        </ul>
        <ul class="nav nav-second-level">
          <li><h5>Trainees <%= Trainee.all.count %></h5></li>
              <% Trainee.all.each do |trainee|%>
                  <div class="user-side">
                    <%= link_to new_user_registration_path do %>
                        <li>
                          <div style="display: inline">
                            <%= image_tag("/assets/profile_thumb.png", size: "40x40", :alt => "rss feed", :style => "float: left" ) %>
                            <%= trainee.user.first_name %> <%= trainee.user.last_name %><br>
                            <small><%= trainee.user.title %></small>
                          </div>
                        </li>
                    <% end %>
                  </div>
              <% end %>

          <li><h5>Observers <%= Observer.all.count %></h5></li>
            <% Observer.all.each do |observer|%>
                  <div class="user-side">
                    <%= link_to new_user_registration_path do %>
                        <li>
                          <div style="display: inline">
                            <%= image_tag("/assets/profile_thumb.png", size: "40x40", :alt => "rss feed", style: "float: left;") %>
                            <%= observer.user.first_name %> <%= observer.user.last_name %><br>
                            <small><%= observer.user.title %></small>
                          </div>
                        </li>
                    <% end %>
                  </div>
            <% end %>

        </ul>
    </div>
</div>
